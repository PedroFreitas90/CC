;
; BIND data file for local loopback interface
;
$TTL    604800
@       IN      SOA     dns.cc.pt. grupo63.cc.pt. (
                              1         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
;
@	IN      NS      dns.cc.pt.
@	IN      NS      dns2.cc.pt.
@	IN      MX      10      mail.cc.pt.
@	IN      MX      20      Servidor2.cc.pt.

mail    IN      A       10.1.1.3
www     IN      A       10.1.1.3
dns     IN      A       10.1.1.1
dns2    IN      A       10.2.2.3
Alfa    IN      A       10.3.3.1
Beta    IN      A       10.3.3.2
Gama    IN      A       10.3.3.3
Urano   IN      A       10.2.2.3
Neptuno IN      A       10.2.2.2
Plutao  IN      A       10.2.2.1
Servidor1	IN	A	10.1.1.1
Servidor2	IN	A	10.1.1.2
Servidor3	IN	A	10.1.1.3
Cliente3        IN      A       10.4.4.3
Cliente2        IN      A       10.4.4.2
Cliente1        IN      A       10.4.4.1

Grupo63 IN      CNAME   Cliente1
pop	IN	CNAME	Servidor2
imap	IN	CNAME	Servidor2
